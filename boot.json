{
    "name": "ARedSecret",
    "version": "-",
    "additionFile": [
        "localisation/zh-CN.json"
    ],
    "additionDir": [
        "assets"
    ],
    "scriptFileList_inject_early": [
        "javascript/utils-earlyload.js"
    ],
    "scriptFileList_earlyload": [],
    "scriptFileList_preload": [],
    "addonPlugin": [
        {
            "modName": "BeautySelectorAddon",
            "addonName": "BeautySelectorAddon",
            "modVersion": "^2.0.0",
            "params": {
                "type": "default",
                "imgFileList": [
                    "assets/icons/books/orchidtransfigurations1_latin.png",
                    "assets/icons/books/orchidtransfigurations1.png",
                    "assets/icons/books/skeletonsongs.png"
                ]
            }
        },
        {
            "modName": "TweeReplacer",
            "addonName": "TweeReplacerAddon",
            "modVersion": "^1.2.0",
            "params": [
                {
                    "passage": "Characteristics",
                    "findRegex": "(?<=<<characteristic-box _housekeepingConfig>>)",
                    "replace": "<<characteristic-Languages>>"
                },
                {
                    "passage": "Bedroom",
                    "findRegex": "(?<=<<deskText \"noicon\">>)",
                    "replace": "<<bookshelfText>><br><br>"
                },
                {
                    "passage": "School Library",
                    "findRegex": "(?<=oliveExitPassage.*\n\t+<br>\n\t+<</if>>)",
                    "replace": "<<schoolLibNewBook>>"
                }
            ]
        }
    ],
    "dependenceInfo": [
        {
            "modName": "BeautySelectorAddon",
            "version": "^2.0.0"
        },
        {
            "modName": "TweeReplacer",
            "version": "^1.2.0"
        }
    ]
}