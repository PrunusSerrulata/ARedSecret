{
    "name": "ARedSecret",
    "version": "-",
    "additionFile": [
        "localisation/zh-CN.json"
    ],
    "scriptFileList_inject_early": [
        "javascript/utils.js"
    ],
    "scriptFileList_earlyload": [],
    "scriptFileList_preload": [],
    "addonPlugin": [
        {
            "modName": "TweeReplacer",
            "addonName": "TweeReplacerAddon",
            "modVersion": "^1.2.0",
            "params": [
                {
                    "passage": "Characteristics",
                    "findRegex": "(?<=<<characteristic-box _housekeepingConfig>>)",
                    "replace": "<<characteristic-Languages>>"
                },
                {
                    "passage": "Bedroom",
                    "findRegex": "(?<=<<deskText \"noicon\">>)",
                    "replace": "<<bookshelfText>><br><br>"
                },
                {
                    "passage": "School Library",
                    "findRegex": "(?<=oliveExitPassage.*\n\t+<br>\n\t+<</if>>)",
                    "replace": "<<schoolLibNewBook>>"
                }
            ]
        }
    ],
    "dependenceInfo": [
        {
            "modName": "TweeReplacer",
            "version": "^1.2.0"
        }
    ]
}